<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Version>0.10.1</Version>
    <PackageId>Cvl.ApplicationServer</PackageId>
    <AssemblyName>Cvl.ApplicationServer</AssemblyName>
    <RootNamespace>Cvl.ApplicationServer</RootNamespace>
    <Authors>Cvlang Organization</Authors>
    <Company>Cvlang OrganizationCvlang Organization</Company>
    <Description>.Net Application Server</Description>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageProjectUrl>https://github.com/cv-lang/application-server</PackageProjectUrl>
    <RepositoryType></RepositoryType>
    <RepositoryUrl>https://github.com/cv-lang/application-server</RepositoryUrl>
    <PackageTags>C#, .net, application server</PackageTags>
    <PackageIcon>cvlogo.png</PackageIcon>
  </PropertyGroup>

  <ItemGroup>
    <None Include="Icons\cvlogo.png">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="SharpSerializer" Version="3.0.1" />
  </ItemGroup>

</Project>
